@model List<login.Models.EmpModel>
@{
    ViewBag.Title = "show";
}
<div id="page-wrapper">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">Tables</h1>
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <!-- /.row -->
        <div class="row">
            <div class="col-lg-12">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        DataTables Advanced Tables
                    </div>
                    <!-- /.panel-heading -->
                    <div class="panel-body">
                        <div class="table-responsive">
                            <table class="table table-bordered table-hover table-responsive">
                                <tr>
                                    <th>Id</th>
                                    <th>Name</th>
                                    <th>Email</th>
                                    <th>Mob</th>
                                    <th>Department</th>
                                    <th>Companey</th>
                                    <th>Delete</th>
                                    <th>Edit</th>
                                </tr>
                                @foreach (var it in Model)
                                {
                                    <tr>
                                        <td>@it.Id</td>
                                        <td>@it.Name</td>
                                        <td>@it.Email</td>
                                        <td>@it.Mob</td>
                                        <td>@it.Department</td>
                                        <td>@it.Companey</td>
                                        <td><a href="@Url.Action("Delete","Home",new {it.Id})" class="btn btn-danger">Delete</a></td>
                                        <td><a href="@Url.Action("Edit","Home",new {it.Id})" class="btn btn-warning">Edit</a></td>
                                    </tr>
                                }

                            </table>




                        </div>
                        <!-- /.table-responsive -->
                    </div>
                    <!-- /.panel-body -->
                </div>
                <!-- /.panel -->
            </div>
            <!-- /.col-lg-6 -->
        </div>
        <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
</div>